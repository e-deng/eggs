{
  "name": "eggs",
  "version": "0.1.0",
  "description": "A beautiful, interactive showcase of Taylor Swift Easter eggs and hidden clues discovered by Swifties around the world",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "clean": "rm -rf node_modules client/node_modules",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "taylor-swift",
    "easter-eggs",
    "swifties",
    "express",
    "react",
    "javascript",
    "supabase",
    "tailwindcss"
  ],
  "author": "e-deng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-deng/eggs.git"
  },
  "bugs": {
    "url": "https://github.com/e-deng/eggs/issues"
  },
  "homepage": "https://github.com/e-deng/eggs#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.55.0",
    "bcrypt": "6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  }
}
